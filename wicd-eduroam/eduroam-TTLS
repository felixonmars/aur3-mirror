name = eduroam-TTLS
author = Oliver Wiedemann
version = 3
require identity *Identity password *Password cert *Cert
protected password *Password
-----
ctrl_interface=/var/run/wpa_supplicant
network={
        ssid=$_ESSID
        scan_ssid=$_SCAN
        proto=WPA WPA2
        key_mgmt=WPA-EAP
        pairwise=CCMP TKIP
        eap=TTLS
        phase2="auth=PAP"
        identity="$_IDENTITY"
        password="$_PASSWORD"
	ca_cert="$_CERT"
}
