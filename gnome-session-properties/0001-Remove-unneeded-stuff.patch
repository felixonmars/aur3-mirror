-
-        if (!prop || strcmp (prop->type, SmCARD8) != 0) {
-                return GSM_CLIENT_RESTART_IF_RUNNING;
-        }
-
-        switch (((unsigned char *)prop->vals[0].value)[0]) {
-        case SmRestartIfRunning:
-                hint = GSM_CLIENT_RESTART_IF_RUNNING;
-                break;
-        case SmRestartAnyway:
