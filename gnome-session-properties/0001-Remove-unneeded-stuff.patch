-static void
-drop_system_inhibitor (GsmSystemd *manager)
-{
-        if (manager->priv->inhibit_fd != -1) {
-                g_debug ("GsmSystemd: Dropping system inhibitor");
-                close (manager->priv->inhibit_fd);
-                manager->priv->inhibit_fd = -1;
-        }
