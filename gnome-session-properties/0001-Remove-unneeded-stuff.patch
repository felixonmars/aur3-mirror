-    {
-      g_print ("fork failed\n");
-      exit (1);
-    }
-
-  if (pid == 0)
-    {
-      execvp (*argv, argv + 1);
-      g_print (_("Failed to execute %s\n"), *argv);
-      exit (1);
-    }
-
-  do
-    {
-      if (waitpid (pid, &status, 0) == -1)
-        {
-          g_print ("waitpid failed\n");
-          exit (1);
-        }
-
-      if (WIFEXITED (status))
-        exit (WEXITSTATUS (status));
-
-    } while (!WIFEXITED (status) && ! WIFSIGNALED(status));
