-int
-main (int argc, char **argv)
-{
-        GdkDisplay *display = NULL;
-        int estatus;
-        char *child_argv[] = { LIBEXECDIR "/gnome-session-check-accelerated-helper", NULL };
-        Window rootwin;
-        glong is_accelerated, is_software_rendering;
