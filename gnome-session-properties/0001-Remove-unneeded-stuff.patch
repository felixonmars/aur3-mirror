-                if (type == XA_CARDINAL) {
-                        glong *is_accelerated_ptr = (glong*) data;
-
-                        if (*is_accelerated_ptr == ACCEL_CHECK_RUNNING) {
-                                /* Test in progress, wait */
-                                if (wait_for_property_notify ())
