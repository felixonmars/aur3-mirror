-static GdkFilterReturn
-property_notify_filter (GdkXEvent *xevent,
-                        GdkEvent  *event,
-                        gpointer   data)
-{
-        XPropertyEvent *ev = xevent;
-
-        if (ev->type == PropertyNotify && ev->atom == is_accelerated_atom) {
-                property_changed = TRUE;
-                gtk_main_quit ();
-        }
