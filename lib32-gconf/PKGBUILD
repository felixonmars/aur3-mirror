pkgname=lib32-gconf
pkgver=2.32.0
pkgrel32=1
pkgrel=1
pkgdesc="A configuration database system"
arch=('x86_64')
license=('LGPL')
groups=('lib32')
depends=('lib32-orbit2' 'lib32-gtk2' 'lib32-libxml2')
url="http://www.gnome.org"
source=("ftp://ftp.archlinux.org/extra/os/i686/${pkgname/lib32-/}-${pkgver}-${pkgrel32}-i686.pkg.tar.xz")
md5sums=('13838365afd788a18e2b051a19e9bda0')

build() {
  mkdir -p $pkgdir/opt/lib32/usr/lib/
  cp -R $srcdir/usr/lib/*.so* $pkgdir/opt/lib32/usr/lib
}

