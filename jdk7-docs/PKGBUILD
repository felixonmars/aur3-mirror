# Maintainer: M0Rf30

pkgname=jdk7-docs
_major=7
_minor=60
_build=b19
pkgver=${_major}.${_minor}
pkgrel=1
pkgdesc='Java SE Development Kit Documentation 7'
arch=('any')
url='http://www.oracle.com/technetwork/java/index.html'
license=('custom')
options=('docs' '!strip')
source=("http://download.oracle.com/otn-pub/java/jdk/7u${_minor}-${_build}/jdk-7u${_minor}-apidocs.zip")
conflicts=('jdk-docs')

package(){
  cd "$srcdir/docs"
  mkdir -p "$pkgdir/usr/share/doc/java7" 
  cp -rf * "$pkgdir/usr/share/doc/java7" 
}

sha256sums=('6dffd325e1d1f28cda64a84c42adad14f1bfc8220471494dac14badf275cdb88')

