pkgrel=1
pkgdesc=""
arch=('i686' 'x86_64')
url="http://www.vanheusden.com/ved/"
license=('GPL')
groups=()
depends=()
makedepends=()
optdepends=()
provides=()
conflicts=()
replaces=()
backup=()
options=()
install=
changelog=
source=(http://www.vanheusden.com/ved/$pkgname-$pkgver.tgz)
noextract=()
md5sums=('dab5bb2687bc43ed7e51962de904fd4e')

build() {
  cd "$srcdir/$pkgname-$pkgver"
  make
}

package() {
  cd "$srcdir/$pkgname-$pkgver"
  mkdir -p $pkgdir/usr/bin/
  cp video_entropyd $pkgdir/usr/bin/
}
