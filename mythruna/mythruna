#!/bin/sh
if [ ! -d /home/$USER/.mythruna ]; then
mkdir -p /home/$USER/.mythruna
fi;
cd /home/$USER/.mythruna
LD_LIBRARY_PATH="$LD_LIBRARY_PATH:$JAVA_HOME/lib/amd64/:$JAVA_HOME/lib/i386/:$JAVA_HOME/jre/lib/amd64/:$JAVA_HOME/jre/lib/i386/" java -XX:MaxDirectMemorySize=1024m -Xmx512m -jar /usr/share/mythruna/20120627/Mythruna-20120627.jar
