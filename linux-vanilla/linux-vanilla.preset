# mkinitcpio preset file for linux-vanilla

########################################
# DO NOT EDIT THIS LINE:
source /etc/mkinitcpio.d/linux-vanilla.kver
########################################
ALL_config="/etc/mkinitcpio.conf"

PRESETS=('default' 'fallback')

#default_config="/etc/mkinitcpio.conf"
default_image="/boot/initramfs-linux-vanilla.img"
#default_options=""

#fallback_config="/etc/mkinitcpio.conf"
fallback_image="/boot/initramfs-linux-vanilla-fallback.img" 
fallback_options="-S autodetect"