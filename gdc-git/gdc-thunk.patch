+        TREE_ASM_WRITTEN (thunk_decl) = 1;
+        //node->thunk.thunk_p = false;
+        //node->analyzed = false;
+    } else
+    {    
+        thunk_node = cgraph_add_thunk (funcn, thunk_decl, thunk_decl,
+                                       this_adjusting, fixed_offset, virtual_value, 0, alias);
+    }
 
