_all_aur="n"
# If you want to use the possibly same packages from official repo, keep _all_aur="n".
# To use packages from AUR only, edit it as _all_aur="y".

pkgname=xfce4-devel-meta
pkgver=4.10pre2
pkgrel=1
pkgdesc="Latest applications of Xfce (Meta package)"
arch=('any')
license=('GPL')
url="http://www.xfce.org/"

depends=(
    "gtk2-xfce-engine>=2.99.2"
    "thunar-devel>=1.3.2"
    "xfce4-panel-devel>=4.9.2"
    "xfce4-session-devel>=4.9.1"
    "xfce4-settings-devel>=4.9.5"
    "xfdesktop-devel>=4.9.3"
    "xfwm4-devel>=4.9."
)

if [ $_all_aur = "y" ]; then
    true && depends=(
    "gtk-xfce-engine-devel>=2.99.3"
    "thunar-devel>=1.3.2"
    "xfce4-panel-devel>=4.9.2"
    "xfce4-session-devel>=4.9.1"
    "xfce4-settings-devel>=4.9.5"
    "xfdesktop-devel>=4.9.3"
    "xfwm4-devel>=4.9."
    )
fi

optdepends=(
    "terminal"
    "tumbler"
    "xfce4-appfinder"
    "xfce4-mixer"
    "xfwm4-themes"
)

conflicts=("xfce-utils")

build() {
    return 0
}

