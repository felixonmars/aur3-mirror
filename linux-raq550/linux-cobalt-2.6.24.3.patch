+//module_init (cobalt_powermode_init);
+//module_exit (cobalt_powermode_exit);
+//#endif
+
+//MODULE_AUTHOR ("Sun Cobalt");
+//MODULE_DESCRIPTION ("Power State Storage");
+//MODULE_LICENSE ("GPL");
+
