# arg 1:  the new package version
post_install()
{
  depmod -A
  echo ""
  echo "==> Remember to add 'asus_eee' to MODULES=() in rc.conf to have it load on boot."
}

# arg 1:  the new package version
# arg 2:  the old package version
post_upgrade()
{
  depmod -A
}

# arg 1:  the old package version
post_remove()
{
  depmod -A
}
