-	pc+= 4;
-	recBSC[psxRegs.code>>26]();
-
-	iFlushRegs();
-	MOV32ItoM((uptr)&psxRegs.pc, branchPC);
-	CALLFunc((uptr)psxBranchTest);
-	/* store cycle */
-	count = (pc - pcold)/4;
-	ADD32ItoM((uptr)&psxRegs.cycle, count);
-	
-	ADD64ItoR(RSP, STACKSIZE);
-
-	// maybe just happened an interruption, check so
