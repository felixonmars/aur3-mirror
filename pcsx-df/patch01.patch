-    // what means the 0x10 and the 0x08 bits? i only saw it used by the bios
-    cdr.Ctrl|=0x18;
-
-#ifdef CDR_LOG
-	CDR_LOG("cdrRead0() Log: CD0 Read: %x\n", cdr.Ctrl);
-#endif
-	return psxHu8(0x1800) = cdr.Ctrl;
-}
