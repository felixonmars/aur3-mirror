-#define CP2_FUNC(f) \
-void gte##f(); \
-static void rec##f() { \
-	if (pc < cop2readypc) idlecyclecount += (cop2readypc - pc)>>2; \
-	iFlushRegs(0); \
-	LIW(0, (u32)psxRegs.code); \
-	STW(0, OFFSET(&psxRegs, &psxRegs.code), GetHWRegSpecial(PSXREGS)); \
-	FlushAllHWReg(); \
-	CALLFunc ((u32)gte##f); \
-	cop2readypc = pc + psxCP2time[_fFunct_(psxRegs.code)]<<2; \
-}
