+// a container for a window with a progress bar.  used for compress and decompress
+class ProgWindow
+{
+public:
+	ProgWindow(const string& text)
+	{
+      {
+         Fl_Window* o = new Fl_Window(220, 50, text.c_str());
+		w = o;
+         prog = new Fl_Progress(0,0,200,20);
