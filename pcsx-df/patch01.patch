+// if there's no CDDA data, just return sensible values
+class NoCDDAData : public CDDAData
+{
+public:
+   NoCDDAData() {}
+   virtual ~NoCDDAData() {}
+   virtual void openFile(const std::string& file) {}
+   virtual int play(const CDTime& startTime) {return 0;}
+   virtual int stop() {return 0;}
+};
