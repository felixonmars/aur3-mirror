+   if (buf == NULL)
+   {
+      toReturn = "pcsx";
+   }
+   else
+   {
+      toReturn = buf;
+   }
+#endif
+
+	// strips all the useless crud from the retuned string
+   toReturn = fl_filename_name(toReturn.c_str());
