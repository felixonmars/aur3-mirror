pkgrel=1
pkgdesc=""
arch=('any')
url="http://ros.org/"
license=('BSD')
groups=()
depends=('python2' 'python2-yaml')
makedepends=()
optdepends=()
provides=()
conflicts=()
replaces=()
backup=()
options=()
install=
source=("http://pr.willowgarage.com/downloads/vcstools/vcstools-$pkgver.tar.gz")
