+      args.Add('files-unwanted', CreateFilesArray)
+    else begin
+      args.Add('files-wanted', CreateFilesArray);
+      args.Add('priority-' + APriority, CreateFilesArray);
+    end;
+    req.Add('arguments', args);
+    args:=RpcObj.SendRequest(req, False);
+    Result:=args<> nil;
+    args.Free;
+  finally
+    req.Free;
+  end;
