-function GetHumanSize(sz: double; RoundTo: integer = 0): string;
-var
-  i: integer;
-begin
-  if sz < 0 then begin
-    Result:='-';
-    exit;
-  end;
-  i:=Low(SizeNames);
-  if RoundTo > 0 then begin
-    Inc(i);
-    sz:=sz/1024;
-  end;
-  while i <= High(SizeNames) do begin
-    if sz < 1024 then
-      break;
-    sz:=sz/1024;
-    Inc(i);
-  end;
-  if (RoundTo = 0) and (i > 3) then
-    RoundTo:=i - 2;
-  Result:=Format('%.' + IntToStr(RoundTo) + 'f %s', [sz, SizeNames[i]]);
-end;
