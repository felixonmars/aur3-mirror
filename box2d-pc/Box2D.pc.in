prefix=@Box2D_PREFIX_PATH@
exec_prefix=${prefix}
libdir=${prefix}/lib
includedir=${prefix}/include

Name: Box2D
Description: A 2D Physics Engine for Games
Version: @BOX2D_VERSION@
URL: http://www.box2d.org
Libs: -L${libdir} -lBox2D @BOX2D_ADDITIONAL_LIBS@
Cflags: -I${includedir} -I${includedir}/Box2D @BOX2D_CFLAGS@
